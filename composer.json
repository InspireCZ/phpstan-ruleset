{
    "type": "library",
    "name": "inspirecz/phpstan-ruleset",
    "description": "INSPIRE phpstan ruleset",
    "license": "MIT",
    "keywords": [
        "phpstan",
        "php",
        "ruleset"
    ],
    "require": {
        "phpstan/phpstan": "^2",
        "phpstan/phpstan-deprecation-rules": "^2",
        "phpstan/phpstan-nette": "^2",
        "phpstan/phpstan-strict-rules": "^2",
        "phpstan/phpstan-dibi": "^2",
        "staabm/phpstan-todo-by": "^0.2"
    },
    "authors": [
        {
            "name": "INSPIRE CZ s.r.o.",
            "email": "firma@inspire.cz",
            "homepage": "https://www.inspire.cz/"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Inspire\\PHPStan\\": "src/"
        }
    },
    "require-dev": {
        "spaze/phpstan-disallowed-calls": "^4"
    }
}
